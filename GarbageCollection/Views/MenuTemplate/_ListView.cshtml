
@model Loader.Models.MenuTemplate
@{
    /**/

    var test = Model;
    Loader.Service.MenuTemplateService menuService = new Loader.Service.MenuTemplateService();
}




<tr class="detailsList">
    <td>
        @Model.MTId
    </td>
    <td class="listDrill" id="@Model.MTId" style="vertical-align:top;">
        <a class="inner listDrill" menu-caption="@Model.MTName" style="cursor:pointer;"> @Model.MTName</a>
    </td>

    <td>
        @menuService.GetUserName(Model.PostedBy)
    </td>
    <td>
        @Model.PostedOn
    </td>
 

    <td><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="btn btn-primary btn-xs editList" id="@Model.MTId" data-title="Edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>
   
</tr>
